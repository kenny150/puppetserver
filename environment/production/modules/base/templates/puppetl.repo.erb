[puppetlabs-products]
name=Puppet Labs Products El <%= @versao %> - $basearch
baseurl=http://yum.puppetlabs.com/el/<%= @versao %>/products/$basearch
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppetlabs
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppet
enabled=1
gpgcheck=1

